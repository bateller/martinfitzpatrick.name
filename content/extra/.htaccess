Options +FollowSymLinks -Indexes 
RewriteEngine On

DirectoryIndex index.html article.html page.html

ErrorDocument 404 /error404/
ErrorDocument 403 /error403/

<Filesmatch "^index.html$">
ExpiresActive On
ExpiresDefault "access plus 1 day"
</Filesmatch>

<Filesmatch "^article.html$">
ExpiresActive On
ExpiresDefault "access plus 1 month"
</Filesmatch>

<Filesmatch "^page.html$">
ExpiresActive On
ExpiresDefault "access plus 1 month"
</Filesmatch>

<Filesmatch "\.(css|jpg|jpeg|png|gif|js|ico)$">
ExpiresActive On
ExpiresDefault "access plus 1 year"
</Filesmatch>

RewriteRule ^index\.html$ / [R=301,L]
RewriteRule ^(.*)/([^/]*)\.html$ /$1/ [R=301,L]

RewriteRule ^posts/(.*) /article/$1 [R=301,L]
RewriteRule ^blog/(.*) /article/$1 [R=301,L]

# Rewrite old urls to new format
RewriteRule ^(methods|applications)/(\d+)/(.*)$ /article/$3 [R=permanent,L]
RewriteRule ^methods/$ / [R=permanent,L]

# Rewrite old urls to current articles
RewriteRule ^methods/37/(.*) /article/measuring-cell-fluorescence-using-imagej/ [R=301,L]
RewriteRule ^methods/1462/(.*) /article/dot-blot-analysis-imagej/ [R=301,L]

